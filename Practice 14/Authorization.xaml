<Window x:Class="Practice_14.Authorization"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practice_14"
        mc:Ignorable="d"
        Title="Authorization" Height="175" Width="350" WindowStartupLocation="CenterScreen" >
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <PasswordBox x:Name="passwordInput" Password="123"/>
            <Button Content="Войти" Click="Enter_Click"/>
            <Button Content="Отмена" Click="Cancel_Click"/>
        </StackPanel>
    </Grid>
</Window>
